{
  "name": "earth-ui",
  "version": "0.5.0",
  "author": "Kimi Gao",
  "description": "Simple Style React UI Components for Web",
  "keywords": [
    "react.js",
    "component",
    "ui"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "module": "es/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cosmos-x/earth-ui.git"
  },
  "bugs": {
    "url": "https://github.com/cosmos-x/earth-ui/issues"
  },
  "homepage": "https://ui.muwenzi.com/",
  "engines": {
    "node": ">=12.0.0",
    "npm": ">=6.9.0"
  },
  "files": [
    "dist",
    "lib"
  ],
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "****** DEV *****": "",
    "create": "node scripts/create.js",
    "dev": "NODE_ENV=development node-dev site/server.js",
    "****** DIST *****": "",
    "build": "node build/build.js",
    "site:dist": "rimraf site/dist && NODE_ENV=production webpack --progress --mode=production --config config/webpack.config.site.js ",
    "****** RELEASE *****": "",
    "release": "npm run lib && npm run dist && standard-version",
    "gh-pages": "npm run site:dist && node scripts/deploy.js",
    "preprod": "npm ci && npm run site:dist",
    "prod": "NODE_ENV=production pm2 start site/server.js --name earth-ui",
    "****** LINT *****": "",
    "fix": "eslint src --fix && eslint site --fix",
    "lint": "eslint src && eslint site",
    "****** TEST *****": "",
    "test": "jest --config=jest.config.json",
    "report-coverage": "codecov",
    "****** OTHERS *****": "",
    "commit": "git-cz"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier-standard",
      "git add"
    ]
  },
  "browserslist": [
    "defaults"
  ],
  "peerDependencies": {
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0",
    "prop-types": ">=15.0.0"
  },
  "dependencies": {
    "classlist": "^1.0.1",
    "classnames": "^2.2.5",
    "focus-trap": "^5.0.2",
    "invariant": "^2.2.2",
    "marked": "^0.7.0",
    "react-avatar-editor": "^10.3.0",
    "react-update": "^0.4.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "@reach/router": "^1.1.1",
    "autoprefixer": "^9.6.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "chalk": "^2.4.2",
    "codecov": "^3.5.0",
    "commitizen": "^2.9.6",
    "compression": "^1.7.2",
    "css-loader": "^3.2.0",
    "cz-conventional-changelog": "^2.1.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.4.0",
    "eslint": "^5.9.0",
    "eslint-config-standard-react": "^7.0.2",
    "eslint-plugin-react": "^7.11.1",
    "express": "^4.16.3",
    "file-loader": "^4.2.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "gulp": "^4.0.2",
    "gulp-replace": "^1.0.0",
    "html-loader": "^0.5.5",
    "husky": "^0.15.0-rc.8",
    "ignore-loader": "^0.1.2",
    "jest": "^24.8.0",
    "jest-css-modules": "^1.1.0",
    "jest-environment-jsdom-global": "^1.0.3",
    "jsx-loader": "^0.13.2",
    "less": "^3.8.1",
    "less-loader": "^5.0.0",
    "lint-staged": "^7.0.0",
    "lodash.template": "^4.4.0",
    "markdown-loader": "^5.1.0",
    "mini-css-extract-plugin": "^0.8.0",
    "multiparty": "^4.1.3",
    "node-dev": "^3.1.3",
    "nprogress": "^0.2.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "pm2": "^2.10.1",
    "postcss-loader": "^3.0.0",
    "prettier-standard": "^9.1.1",
    "prop-types": "^15.6.1",
    "rc-queue-anim": "^1.6.10",
    "rc-scroll-anim": "^2.5.6",
    "react": "^16.6.3",
    "react-copy-to-clipboard": "^5.0.1",
    "react-custom-scrollbars": "^4.2.1",
    "react-dom": "^16.6.3",
    "react-imported-component": "^5.2.3",
    "rimraf": "^2.6.2",
    "standard": "^12.0.1",
    "standard-version": "^4.3.0",
    "style-loader": "^1.0.0",
    "styled-components": "^4.1.1",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^2.1.0",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.7",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-livereload-plugin": "^2.2.0",
    "webpack-merge": "^4.2.2"
  }
}
